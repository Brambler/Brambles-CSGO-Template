App.WindowsCommandLineArgs=-game csgo -console -usercon +game_type {{$game_type}} +game_mode {{$game_mode}} +mapgroup {{$map_group}} -tickrate {{$tickrate}} +map {{$map}} -maxplayers_override {{$playercount}} +sv_setsteamaccount {{$steamserverid}} -port {{$ApplicationPort1}}
